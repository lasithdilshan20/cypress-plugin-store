{
  "name": "cypress-plugin-store",
  "version": "1.3.4",
  "description": "Store values during the executions and retrieve them in any test case in the same spec file.",
  "main": "src",
  "types": "src",
  "files": [
    "src",
    "commands.js"
  ],
  "release": {
    "branches": [
      "1.0",
      {
        "name": "master",
        "channel": "rc",
        "prerelease": "rc"
      }
    ]
  },
  "scripts": {
    "test": "cypress run",
    "start": "npx cypress run --record --key 026ce059-c468-4451-9a3f-eaf42ce89a32",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "semantic-release": "semantic-release",
    "badges": "npx -p dependency-version-badge update-badge cypress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lasithdilshan20/cypress-plugin-store.git"
  },
  "keywords": [
    "cypress",
    "store",
    "variables",
    "cypress.io",
    "cypress-plugin",
    "cypress-store-variables"
  ],
  "author": "Lasithdilshan20",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lasithdilshan20/cypress-plugin-store/issues"
  },
  "homepage": "https://github.com/lasithdilshan20/cypress-plugin-store#readme",
  "dependencies": {
    "prettier": "^2.8.1"
  },
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "cypress": "^13.4.0",
    "semantic-release": "20.0.2"
  }
}
